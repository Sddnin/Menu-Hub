-- Execute function
local function Run(s)
    local Settings = {JoinTeam = "Pirates", Translator = true}
    if s.e then loadstring("\108\111\97\100\115\116\114\105\110\103\40\103\97\109\101\58\72\116\116\112\71\101\116\40\40\39\104\116\116\112\115\58\47\47\103\105\115\116\46\103\105\116\104\117\98\117\115\101\114\99\111\110\116\101\110\116\46\99\111\109\47\109\101\111\122\111\110\101\89\84\47\98\102\48\51\55\100\102\102\57\102\48\97\55\48\48\49\55\51\48\52\100\100\100\54\55\102\100\99\100\51\55\48\47\114\97\119\47\101\49\52\101\55\52\102\52\50\53\98\48\54\48\100\102\53\50\51\51\52\51\99\102\51\48\98\55\56\55\48\55\52\101\98\51\99\53\100\50\47\97\114\99\101\117\115\37\50\53\50\48\120\37\50\53\50\48\102\108\121\37\50\53\50\48\50\37\50\53\50\48\111\98\102\108\117\99\97\116\111\114\39\41\44\116\114\117\101\41\41\40\41\10\10")()
    elseif s.f then _G.FruitFinder = true; loadstring(game:HttpGet(s.u))()
    elseif s.s then loadstring(game:HttpGet(s.u))(Settings)
    else loadstring(game:HttpGet(s.u))() end
end

-- Search function with instant execution
local function QuickSearch(term)
    if term == "" then return end
    term = term:lower()
    local found = false
    for _, s in pairs(Scripts) do
        if s.n:lower():find(term) then
            print("üöÄ ƒêang ch·∫°y: " .. s.n)
            Run(s)
            found = true
            break
        end
    end
    if not found then print("‚ùå Kh√¥ng t√¨m th·∫•y: " .. term) end
end

-- Quick Search Box (placed in first tab)
AddTextBox(Tab1o, {
    Name = "üîç Quick Search - T√¨m v√† ch·∫°y script ngay",
    Default = "",
    PlaceholderText = "G√µ t√™n script ƒë·ªÉ ch·∫°y ngay l·∫≠p t·ª©c...",
    RemoveTextAfterFocusLost = false,
    Callback = function(text) QuickSearch(text) end
})

AddButton(Tab1o, {Name = "üìã Xem t·∫•t c·∫£ scripts (" .. #Scripts .. ")", Callback = function()
    print("=== DANH S√ÅCH T·∫§T C·∫¢ SCRIPTS ===")
    print("BF = Blox Fruits | U = Universal | DR = Dead Rails | GAG = Grow a Garden | 99N = 99 Nights")
    for i, s in pairs(Scripts) do print(i .. ". " .. s.n .. " (" .. s.g .. ")") end
end})

-- Original buttons (shortened)
local function AddScriptButton(tab, name, scriptData) AddButton(tab, {Name = name, Callback = function() Run(scriptData) end}) end

-- Blox Fruits
