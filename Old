-- Execute function
local function Run(s)
    local Settings = {JoinTeam = "Pirates", Translator = true}
    if s.e then 
        loadstring("\108\111\97\100\115\116\114\105\110\103\40\103\97\109\101\58\72\116\116\112\71\101\116\40\40\39\104\116\116\112\115\58\47\47\103\105\115\116\46\103\105\116\104\117\98\117\115\101\114\99\111\110\116\101\110\116\46\99\111\109\47\109\101\111\122\111\110\101\89\84\47\98\102\48\51\55\100\102\102\57\102\48\97\55\48\48\49\55\51\48\52\100\100\100\54\55\102\100\99\100\51\55\48\47\114\97\119\47\101\49\52\101\55\52\102\52\50\53\98\48\54\48\100\102\53\50\51\51\52\51\99\102\51\48\98\55\56\55\48\55\52\101\98\51\99\53\100\50\47\97\114\99\101\117\115\37\50\53\50\48\120\37\50\53\50\48\102\108\121\37\50\53\50\48\50\37\50\53\50\48\111\98\102\108\117\99\97\116\111\114\39\41\44\116\114\117\101\41\41\40\41\10\10")()
    elseif s.f then 
        _G.FruitFinder = true
        loadstring(game:HttpGet(s.u))()
    elseif s.s then 
        loadstring(game:HttpGet(s.u))(Settings)
    else 
        loadstring(game:HttpGet(s.u))()
    end
end

-- Variables
local currentTab = "Blox fruits"
local allTabs = {"Blox fruits", "Misc", "99 night in the forest", "Dead rails", "Grow a garden", "üîç Search"}
local searchMode = false
local searchResults = {}

-- Create Tab Button
local function CreateTabButton(tabName)
    local tabButton = Instance.new("TextButton")
    tabButton.Name = tabName
    tabButton.Size = UDim2.new(1, 0, 0, 40)
    tabButton.BackgroundColor3 = currentTab == tabName and Color3.fromRGB(70, 70, 70) or Color3.fromRGB(45, 45, 45)
    tabButton.BorderSizePixel = 0
    tabButton.Text = tabName
    tabButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    tabButton.TextSize = 14
    tabButton.Font = Enum.Font.Gotham
    tabButton.Parent = tabContainer
    
    local tabCorner = Instance.new("UICorner")
    tabCorner.CornerRadius = UDim.new(0, 6)
    tabCorner.Parent = tabButton
    
    tabButton.MouseButton1Click:Connect(function()
        currentTab = tabName
        UpdateDisplay()
        UpdateTabColors()
    end)
    
    return tabButton
end

-- Create Script Button
local function CreateScriptButton(script)
    local scriptButton = Instance.new("TextButton")
    scriptButton.Name = script.n
    scriptButton.Size = UDim2.new(0, 200, 0, 35)
    scriptButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
    scriptButton.BorderSizePixel = 0
    scriptButton.Text = script.n
    scriptButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    scriptButton.TextSize = 12
    scriptButton.Font = Enum.Font.Gotham
    scriptButton.Parent = scriptContainer
    
    local scriptCorner = Instance.new("UICorner")
    scriptCorner.CornerRadius = UDim.new(0, 6)
    scriptCorner.Parent = scriptButton
    
    -- Hover effect
    scriptButton.MouseEnter:Connect(function()
        scriptButton.BackgroundColor3 = Color3.fromRGB(70, 70, 70)
    end)
    
    scriptButton.MouseLeave:Connect(function()
        scriptButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
    end)
    
    scriptButton.MouseButton1Click:Connect(function()
        Run(script)
        print("üöÄ ƒêang ch·∫°y: " .. script.n)
    end)
    
    return scriptButton
end

-- Search functionality
local function SearchScripts(searchTerm)
    searchResults = {}
    if searchTerm == "" then return searchResults end
    searchTerm = searchTerm:lower()
    
    for _, script in pairs(Scripts) do
        if script.n:lower():find(searchTerm) or script.g:lower():find(searchTerm) then
            table.insert(searchResults, script)
        end
    end
    return searchResults
end

-- Create Search UI
local function CreateSearchUI()
    -- Clear existing content
    for _, child in pairs(scriptContainer:GetChildren()) do
        if child:IsA("TextButton") or child:IsA("TextBox") then
            child:Destroy()
        end
    end
    
    -- Search TextBox
    local searchBox = Instance.new("TextBox")
    searchBox.Name = "SearchBox"
    searchBox.Size = UDim2.new(1, -10, 0, 35)
    searchBox.Position = UDim2.new(0, 5, 0, 5)
    searchBox.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
    searchBox.BorderSizePixel = 0
    searchBox.PlaceholderText = "T√¨m ki·∫øm script..."
    searchBox.Text = ""
    searchBox.TextColor3 = Color3.fromRGB(255, 255, 255)
    searchBox.TextSize = 14
    searchBox.Font = Enum.Font.Gotham
    searchBox.Parent = scriptDisplay
    
    local searchBoxCorner = Instance.new("UICorner")
    searchBoxCorner.CornerRadius = UDim.new(0, 6)
    searchBoxCorner.Parent = searchBox
    
    -- Search results container
    local resultsContainer = Instance.new("ScrollingFrame")
    resultsContainer.Name = "ResultsContainer"
    resultsContainer.Size = UDim2.new(1, -10, 1, -50)
    resultsContainer.Position = UDim2.new(0, 5, 0, 45)
    resultsContainer.BackgroundTransparency = 1
    resultsContainer.BorderSizePixel = 0
    resultsContainer.ScrollBarThickness = 8
    resultsContainer.ScrollBarImageColor3 = Color3.fromRGB(255, 255, 255)
    resultsContainer.Parent = scriptDisplay
    
    local resultsLayout = Instance.new("UIListLayout")
    resultsLayout.SortOrder = Enum.SortOrder.LayoutOrder
    resultsLayout.Padding = UDim.new(0, 5)
    resultsLayout.Parent = resultsContainer
    
    -- Search functionality
    searchBox.Changed:Connect(function(property)
        if property == "Text" then
            -- Clear previous results
            for _, child in pairs(resultsContainer:GetChildren()) do
                if child:IsA("TextButton") then
                    child:Destroy()
                end
            end
            
            local results = SearchScripts(searchBox.Text)
            
            if #results > 0 then
                for _, script in pairs(results) do
                    local resultButton = Instance.new("TextButton")
                    resultButton.Size = UDim2.new(1, 0, 0, 35)
                    resultButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
                    resultButton.BorderSizePixel = 0
                    resultButton.Text = script.n .. " (" .. script.g .. ")"
                    resultButton.TextColor3 = Color3.fromRGB(255, 255, 255)
                    resultButton.TextSize = 12
                    resultButton.Font = Enum.Font.Gotham
                    resultButton.Parent = resultsContainer
                    
                    local resultCorner = Instance.new("UICorner")
                    resultCorner.CornerRadius = UDim.new(0, 6)
                    resultCorner.Parent = resultButton
                    
                    resultButton.MouseEnter:Connect(function()
                        resultButton.BackgroundColor3 = Color3.fromRGB(70, 70, 70)
                    end)
                    
                    resultButton.MouseLeave:Connect(function()
                        resultButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
                    end)
                    
                    resultButton.MouseButton1Click:Connect(function()
                        Run(script)
                        print("üöÄ ƒêang ch·∫°y: " .. script.n)
                    end)
                end
            else
                local noResultLabel = Instance.new("TextLabel")
                noResultLabel.Size = UDim2.new(1, 0, 0, 35)
                noResultLabel.BackgroundTransparency = 1
                noResultLabel.Text = searchBox.Text == "" and "Nh·∫≠p t·ª´ kh√≥a ƒë·ªÉ t√¨m ki·∫øm..." or "Kh√¥ng t√¨m th·∫•y k·∫øt qu·∫£ n√†o"
                noResultLabel.TextColor3 = Color3.fromRGB(150, 150, 150)
                noResultLabel.TextSize = 12
                noResultLabel.Font = Enum.Font.Gotham
                noResultLabel.Parent = resultsContainer
            end
            
            resultsContainer.CanvasSize = UDim2.new(0, 0, 0, resultsLayout.AbsoluteContentSize.Y)
        end
    end)
end
-- Update Display
function UpdateDisplay()
    if currentTab == "üîç Search" then
        CreateSearchUI()
        return
    end
    
    for _, child in pairs(scriptContainer:GetChildren()) do
        if child:IsA("TextButton") then
            child:Destroy()
        end
    end
    
    -- Clear search UI if switching from search tab
    for _, child in pairs(scriptDisplay:GetChildren()) do
        if child.Name == "SearchBox" or child.Name == "ResultsContainer" then
            child:Destroy()
        end
    end
    
    for _, script in pairs(Scripts) do
        if script.g == currentTab then
            CreateScriptButton(script)
        end
    end
    
    scriptContainer.CanvasSize = UDim2.new(0, 0, 0, scriptLayout.AbsoluteContentSize.Y)
end

-- Update Tab Colors
function UpdateTabColors()
    for _, child in pairs(tabContainer:GetChildren()) do
        if child:IsA("TextButton") then
            child.BackgroundColor3 = child.Name == currentTab and Color3.fromRGB(70, 70, 70) or Color3.fromRGB(45, 45, 45)
        end
    end
end

-- Initialize UI
for _, tabName in pairs(allTabs) do
    CreateTabButton(tabName)
end

UpdateDisplay()

-- Update canvas sizes
tabContainer.CanvasSize = UDim2.new(0, 0, 0, tabLayout.AbsoluteContentSize.Y)

-- Button functionality
searchButton.MouseButton1Click:Connect(function()
    currentTab = "üîç Search"
    UpdateDisplay()
    UpdateTabColors()
end)

closeButton.MouseButton1Click:Connect(function()
    screenGui:Destroy()
end)

local minimized = false
minimizeButton.MouseButton1Click:Connect(function()
    minimized = not minimized
    contentFrame.Visible = not minimized
    mainFrame.Size = minimized and UDim2.new(0, 600, 0, 50) or UDim2.new(0, 600, 0, 400)
    minimizeButton.Text = minimized and "+" or "‚àí"
end)

print("üéâ Mila Hub V1 ƒë√£ t·∫£i th√†nh c√¥ng!")
print("üìã T·ªïng c·ªông: " .. #Scripts .. " scripts")
print("üîç Click n√∫t Search ho·∫∑c tab ƒë·ªÉ t√¨m ki·∫øm scripts")