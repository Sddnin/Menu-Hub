-- Status notification
local statusNotification = Instance.new("Frame")
statusNotification.Name = "StatusNotification"
statusNotification.Size = UDim2.new(0, 400, 0, 60)
statusNotification.Position = UDim2.new(0.5, -200, 0, -70)
statusNotification.BackgroundColor3 = Color3.fromRGB(46, 204, 113)
statusNotification.BorderSizePixel = 0
statusNotification.Visible = false
statusNotification.Parent = mainContainer

local statusCorner = Instance.new("UICorner")
statusCorner.CornerRadius = UDim.new(0, 15)
statusCorner.Parent = statusNotification

local statusIcon = Instance.new("TextLabel")
statusIcon.Size = UDim2.new(0, 40, 1, 0)
statusIcon.Position = UDim2.new(0, 0, 0, 0)
statusIcon.BackgroundTransparency = 1
statusIcon.Text = "‚úÖ"
statusIcon.TextColor3 = Color3.fromRGB(255, 255, 255)
statusIcon.TextSize = 24
statusIcon.Font = Enum.Font.Gotham
statusIcon.Parent = statusNotification

local statusText = Instance.new("TextLabel")
statusText.Size = UDim2.new(1, -40, 1, 0)
statusText.Position = UDim2.new(0, 40, 0, 0)
statusText.BackgroundTransparency = 1
statusText.Text = "Link copied successfully!"
statusText.TextColor3 = Color3.fromRGB(255, 255, 255)
statusText.TextSize = 16
statusText.Font = Enum.Font.GothamSemibold
statusText.TextXAlignment = Enum.TextXAlignment.Left
statusText.Parent = statusNotification

-- Variables
local currentTab = "Global"
local isMinimized = false

-- Functions
local function showNotification(text, isSuccess)
    statusText.Text = text
    statusNotification.BackgroundColor3 = isSuccess and Color3.fromRGB(46, 204, 113) or Color3.fromRGB(231, 76, 60)
    statusIcon.Text = isSuccess and "‚úÖ" or "‚ùå"
    statusNotification.Visible = true
    
    local slideIn = TweenService:Create(statusNotification, TweenInfo.new(0.3, Enum.EasingStyle.Back), {
        Position = UDim2.new(0.5, -200, 0, 20)
    })
    slideIn:Play()
    
    spawn(function()
        wait(3)
        local slideOut = TweenService:Create(statusNotification, TweenInfo.new(0.3, Enum.EasingStyle.Back), {
            Position = UDim2.new(0.5, -200, 0, -70)
        })
        slideOut:Play()
        slideOut.Completed:Connect(function()
            statusNotification.Visible = false
        end)
    end)
end

local function createModernExecutorCard(executorInfo, index)
    local card = Instance.new("Frame")
    card.Name = executorInfo.name .. "Card"
    card.Size = UDim2.new(0, 180, 0, 120)
    card.Position = UDim2.new(0, (index % 4) * 190 + 10, 0, math.floor(index / 4) * 130 + 10)
    card.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    card.BackgroundTransparency = 0.1
    card.BorderSizePixel = 0
    card.Parent = contentArea
    
    local cardCorner = Instance.new("UICorner")
    cardCorner.CornerRadius = UDim.new(0, 15)
    cardCorner.Parent = card
    
    local cardStroke = Instance.new("UIStroke")
    cardStroke.Color = executorInfo.color
    cardStroke.Transparency = 0.5
    cardStroke.Thickness = 2
    cardStroke.Parent = card
    
    -- Card gradient
    local cardGradient = Instance.new("UIGradient")
    cardGradient.Color = ColorSequence.new{
        ColorSequenceKeypoint.new(0.0, Color3.fromRGB(255, 255, 255)),
        ColorSequenceKeypoint.new(1.0, executorInfo.color)
    }
    cardGradient.Transparency = NumberSequence.new{
        NumberSequenceKeypoint.new(0.0, 0.9),
        NumberSequenceKeypoint.new(1.0, 0.95)
    }
    cardGradient.Rotation = 45
    cardGradient.Parent = card
    
    -- Icon
    local icon = Instance.new("TextLabel")
    icon.Size = UDim2.new(0, 40, 0, 40)
    icon.Position = UDim2.new(0, 15, 0, 15)
    icon.BackgroundTransparency = 1
    icon.Text = executorInfo.icon
    icon.TextColor3 = executorInfo.color
    icon.TextSize = 28
    icon.Font = Enum.Font.Gotham
    icon.Parent = card
    
    -- Name
    local nameLabel = Instance.new("TextLabel")
    nameLabel.Size = UDim2.new(1, -70, 0, 25)
    nameLabel.Position = UDim2.new(0, 60, 0, 15)
    nameLabel.BackgroundTransparency = 1
    nameLabel.Text = executorInfo.name
    nameLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    nameLabel.TextSize = 16
    nameLabel.Font = Enum.Font.GothamBold
    nameLabel.TextXAlignment = Enum.TextXAlignment.Left
    nameLabel.Parent = card
    
    -- Status
    local statusLabel = Instance.new("TextLabel")
    statusLabel.Size = UDim2.new(1, -70, 0, 20)
    statusLabel.Position = UDim2.new(0, 60, 0, 35)
    statusLabel.BackgroundTransparency = 1
    statusLabel.Text = "Ready to download"
    statusLabel.TextColor3 = Color3.fromRGB(200, 200, 200)
    statusLabel.TextSize = 12
    statusLabel.Font = Enum.Font.Gotham
    statusLabel.TextXAlignment = Enum.TextXAlignment.Left
    statusLabel.Parent = card
    
    -- Download button
    local downloadBtn = Instance.new("TextButton")
    downloadBtn.Size = UDim2.new(1, -20, 0, 35)
    downloadBtn.Position = UDim2.new(0, 10, 1, -45)
    downloadBtn.BackgroundColor3 = executorInfo.color
    downloadBtn.BorderSizePixel = 0
    downloadBtn.Text = "üì• Copy Link"
    downloadBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
    downloadBtn.TextSize = 14
    downloadBtn.Font = Enum.Font.GothamSemibold
    downloadBtn.Parent = card
    
    local btnCorner = Instance.new("UICorner")
    btnCorner.CornerRadius = UDim.new(0, 8)
    btnCorner.Parent = downloadBtn
    
    -- Hover effects
    local hoverTween
    card.MouseEnter:Connect(function()
        if hoverTween then hoverTween:Cancel() end
        hoverTween = TweenService:Create(card, TweenInfo.new(0.2, Enum.EasingStyle.Quad), {
            Size = UDim2.new(0, 185, 0, 125)
        })
        hoverTween:Play()
        
        TweenService:Create(cardStroke, TweenInfo.new(0.2, Enum.EasingStyle.Quad), {
            Transparency = 0.2
        }):Play()
    end)
    
    card.MouseLeave:Connect(function()
        if hoverTween then hoverTween:Cancel() end
        hoverTween = TweenService:Create(card, TweenInfo.new(0.2, Enum.EasingStyle.Quad), {
            Size = UDim2.new(0, 180, 0, 120)
        })
        hoverTween:Play()
        
        TweenService:Create(cardStroke, TweenInfo.new(0.2, Enum.EasingStyle.Quad), {
            Transparency = 0.5
        }):Play()
    end)
    
    -- Button click effect
    downloadBtn.MouseButton1Click:Connect(function()
        -- Click animation
        local clickTween = TweenService:Create(downloadBtn, TweenInfo.new(0.1, Enum.EasingStyle.Quad), {
            Size = UDim2.new(1, -22, 0, 33)
        })
        clickTween:Play()
        
        clickTween.Completed:Connect(function()
            TweenService:Create(downloadBtn, TweenInfo.new(0.1, Enum.EasingStyle.Quad), {
                Size = UDim2.new(1, -20, 0, 35)
            }):Play()
        end)
        
        -- Copy link
        local success = pcall(function()
            setclipboard(executorInfo.link)
        end)
        
        if success then
            showNotification(executorInfo.name .. " link copied to clipboard!", true)
            statusLabel.Text = "‚úÖ Link copied!"
            statusLabel.TextColor3 = Color3.fromRGB(46, 204, 113)
        else
            showNotification("Failed to copy: " .. executorInfo.link, false)
            statusLabel.Text = "‚ùå Copy failed"
            statusLabel.TextColor3 = Color3.fromRGB(231, 76, 60)
        end
        
        spawn(function()
            wait(2)
            statusLabel.Text = "Ready to download"
            statusLabel.TextColor3 = Color3.fromRGB(200, 200, 200)
        end)
        
        print(executorInfo.name .. " clicked! Link: " .. executorInfo.link)
    end)
    
    return card
end

local function switchTab(tabName)
    -- Clear existing cards
    for _, child in pairs(contentArea:GetChildren()) do
        if child:IsA("Frame") and child.Name:find("Card") then
            child:Destroy()
        end
    end
    
    -- Update tab appearance with smooth animations
    if tabName == "Global" then
        TweenService:Create(globalTab, TweenInfo.new(0.3, Enum.EasingStyle.Quad), {
            BackgroundColor3 = Color3.fromRGB(138, 43, 226),
            TextColor3 = Color3.fromRGB(255, 255, 255)
        }):Play()
        
        TweenService:Create(vngTab, TweenInfo.new(0.3, Enum.EasingStyle.Quad), {
            BackgroundColor3 = Color3.fromRGB(255, 255, 255),
            BackgroundTransparency = 0.3,
            TextColor3 = Color3.fromRGB(100, 100, 100)
        }):Play()
        
        currentTab = "Global"
        
    elseif tabName == "VNG" then
        TweenService:Create(vngTab, TweenInfo.new(0.3, Enum.EasingStyle.Quad), {
            BackgroundColor3 = Color3.fromRGB(255, 69, 0),
            BackgroundTransparency = 0,
            TextColor3 = Color3.fromRGB(255, 255, 255)
        }):Play()
        
        TweenService:Create(globalTab, TweenInfo.new(0.3, Enum.EasingStyle.Quad), {
            BackgroundColor3 = Color3.fromRGB(255, 255, 255),
            BackgroundTransparency = 0.3,
            TextColor3 = Color3.fromRGB(100, 100, 100)
        }):Play()
        
        currentTab = "VNG"
    end
    
    -- Create cards for current tab
    local data = executorData[currentTab]
    for i, executor in ipairs(data) do
        createModernExecutorCard(executor, i - 1)
    end
    
    -- Update canvas size
    local rows = math.ceil(#data / 4)
    contentArea.CanvasSize = UDim2.new(0, 0, 0, rows * 130 + 20)
end

-- Event handlers
globalTab.MouseButton1Click:Connect(function()
    switchTab("Global")
end)

vngTab.MouseButton1Click:Connect(function()
    switchTab("VNG")
end)

closeButton.MouseButton1Click:Connect(function()
    local closeTween = TweenService:Create(mainContainer, TweenInfo.new(0.5, Enum.EasingStyle.Back), {
        Size = UDim2.new(0, 0, 0, 0)
    })
    closeTween:Play()
    
    TweenService:Create(blurFrame, TweenInfo.new(0.3, Enum.EasingStyle.Quad), {
        BackgroundTransparency = 1
    }):Play()
    
    closeTween.Completed:Connect(function()
        screenGui:Destroy()
    end)
end)

minimizeButton.MouseButton1Click:Connect(function()
    isMinimized = not isMinimized
    
    if isMinimized then
        TweenService:Create(mainContainer, TweenInfo.new(0.3, Enum.EasingStyle.Quad), {
            Size = UDim2.new(0, 300, 0, 80),
            Position = UDim2.new(0.5, -150, 0.5, -40)
        }):Play()
        
        minimizeButton.Text = "+"
        tabContainer.Visible = false
        contentArea.Visible = false
    else
        TweenService:Create(mainContainer, TweenInfo.new(0.3, Enum.EasingStyle.Quad), {
            Size = UDim2.new(0, 800, 0, 500),
            Position = UDim2.new(0.5, -400, 0.5, -250)
        }):Play()
        
        minimizeButton.Text = "‚àí"
        tabContainer.Visible = true
        contentArea.Visible = true
    end
end)

-- Dragging functionality
local dragging = false
local dragStart = nil
local startPos = nil

header.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = true
        dragStart = input.Position
        startPos = mainContainer.Position
    end
end)

header.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement and dragging then
        local delta = input.Position - dragStart
        mainContainer.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
    end
end)

header.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = false
    end
end)

-- Opening animation
mainContainer.Size = UDim2.new(0, 0, 0, 0)
blurFrame.BackgroundTransparency = 1

local openTween = TweenService:Create(mainContainer, TweenInfo.new(0.6, Enum.EasingStyle.Back), {
    Size = UDim2.new(0, 800, 0, 500)
})

local blurTween = TweenService:Create(blurFrame, TweenInfo.new(0.4, Enum.EasingStyle.Quad), {
    BackgroundTransparency = 0.5
})

blurTween:Play()
wait(0.2)
openTween:Play()

openTween.Completed:Connect(function()
    switchTab("Global") -- Initialize with Global tab
    showNotification("üéâ Welcome to Client Hub v2.0!", true)
end)

print("üöÄ Client GUI v2.0 loaded successfully!")