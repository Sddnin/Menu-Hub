--  Client GUI v2.0
-- Enhanced with modern design, animations, and improved functionality

local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local RunService = game:GetService("RunService")

local player = Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

-- T·∫°o ScreenGui v·ªõi backdrop blur
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "ClientGUI_v2"
screenGui.ResetOnSpawn = false
screenGui.Parent = playerGui

-- T·∫°o backdrop blur effect
local blurFrame = Instance.new("Frame")
blurFrame.Name = "BlurBackdrop"
blurFrame.Size = UDim2.new(1, 0, 1, 0)
blurFrame.Position = UDim2.new(0, 0, 0, 0)
blurFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
blurFrame.BackgroundTransparency = 0.5
blurFrame.BorderSizePixel = 0
blurFrame.Parent = screenGui

-- T·∫°o main container v·ªõi glass morphism effect
local mainContainer = Instance.new("Frame")
mainContainer.Name = "MainContainer"
mainContainer.Size = UDim2.new(0, 800, 0, 500)
mainContainer.Position = UDim2.new(0.5, -400, 0.5, -250)
mainContainer.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
mainContainer.BackgroundTransparency = 0.1
mainContainer.BorderSizePixel = 0
mainContainer.Parent = screenGui

-- Glass morphism effect
local containerCorner = Instance.new("UICorner")
containerCorner.CornerRadius = UDim.new(0, 20)
containerCorner.Parent = mainContainer

local containerStroke = Instance.new("UIStroke")
containerStroke.Color = Color3.fromRGB(255, 255, 255)
containerStroke.Transparency = 0.8
containerStroke.Thickness = 2
containerStroke.Parent = mainContainer

-- T·∫°o gradient background
local gradient = Instance.new("UIGradient")
gradient.Color = ColorSequence.new{
    ColorSequenceKeypoint.new(0.0, Color3.fromRGB(138, 43, 226)),
    ColorSequenceKeypoint.new(0.5, Color3.fromRGB(30, 144, 255)),
    ColorSequenceKeypoint.new(1.0, Color3.fromRGB(50, 205, 50))
}
gradient.Rotation = 45
gradient.Parent = mainContainer

-- Animated gradient effect
spawn(function()
    while mainContainer.Parent do
        local tween = TweenService:Create(gradient, TweenInfo.new(3, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, -1, true), {
            Rotation = gradient.Rotation + 360
        })
        tween:Play()
        wait(3)
    end
end)

