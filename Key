-- Editor Functions
executeEditorBtn.MouseButton1Click:Connect(function()
    local code = editorBox.Text
    pcall(function()
        loadstring(code)()
    end)
    TweenService:Create(executeEditorBtn, TweenInfo.new(0.2), {
        BackgroundColor3 = Color3.fromRGB(80, 180, 80)
    }):Play()
    wait(0.3)
    TweenService:Create(executeEditorBtn, TweenInfo.new(0.2), {
        BackgroundColor3 = Color3.fromRGB(100, 200, 100)
    }):Play()
end)

clearEditorBtn.MouseButton1Click:Connect(function()
    editorBox.Text = "-- Write your Lua script here\n"
end)

-- Tab Events
for tabName, tabData in pairs(tabButtons) do
    tabData.button.MouseButton1Click:Connect(function()
        switchTab(tabName)
        if tabName == "Script" then
            updateScriptList("")
        end
    end)
end

-- Search Events
searchBox.Changed:Connect(function(property)
    if property == "Text" then
        updateScriptList(searchBox.Text)
    end
end)

playerSearchBox.Changed:Connect(function(property)
    if property == "Text" then
        updatePlayerList(playerSearchBox.Text)
    end
end)

-- Close/Toggle
closeBtn.MouseButton1Click:Connect(function()
    TweenService:Create(mainFrame, TweenInfo.new(0.3), {
        Size = UDim2.new(0, 0, 0, 0),
        Position = UDim2.new(0.5, 0, 0.5, 0)
    }):Play()
    wait(0.3)
    mainFrame.Visible = false
    toggleButton.Visible = true
    playerFrame.Visible = false
    mainFrame.Size = UDim2.new(0, 380, 0, 270)
    mainFrame.Position = UDim2.new(0.5, -190, 0.5, -135)
end)

toggleButton.MouseButton1Click:Connect(function()
    mainFrame.Visible = true
    toggleButton.Visible = false
    mainFrame.Size = UDim2.new(0, 0, 0, 0)
    mainFrame.Position = UDim2.new(0.5, 0, 0.5, 0)
    TweenService:Create(mainFrame, TweenInfo.new(0.4, Enum.EasingStyle.Back), {
        Size = UDim2.new(0, 380, 0, 270),
        Position = UDim2.new(0.5, -190, 0.5, -135)
    }):Play()
end)

-- Keyboard Shortcut
UserInputService.InputBegan:Connect(function(input, gameProcessed)
    if gameProcessed then return end
    if input.KeyCode == Enum.KeyCode.RightShift then
        if mainFrame.Visible then
            closeBtn.MouseButton1Click()
        else
            toggleButton.MouseButton1Click()
        end
    end
end)

-- Character Respawn
player.CharacterAdded:Connect(function(newCharacter)
    character = newCharacter
    humanoid = character:WaitForChild("Humanoid")
    noclip = false
    infiniteJump = false
    if jumpConnection then
        jumpConnection:Disconnect()
        jumpConnection = nil
    end
    wait(1)
    humanoid.WalkSpeed = originalWalkSpeed
    humanoid.JumpPower = originalJumpPower
end)

