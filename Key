    resetCamera()
end)

createButton("AntiAFKButton", "âš¡ Anti-AFK", UDim2.new(0, 120, 0, 128), function()
    toggleAntiAFK()
    local btn = homeContent:FindFirstChild("AntiAFKButton")
    if btn then
        TweenService:Create(btn, TweenInfo.new(0.3), {
            BackgroundColor3 = Color3.fromRGB(100, 200, 100)
        }):Play()
        btn.Text = "âœ… Anti-AFK ON"
    end
end)

createButton("ResetButton", "ðŸ”„ Reset All", UDim2.new(0, 235, 0, 128), function()
    humanoid.WalkSpeed = originalWalkSpeed
    humanoid.JumpPower = originalJumpPower
    noclip = false
    infiniteJump = false
    antiAFK = false
    if jumpConnection then
        jumpConnection:Disconnect()
        jumpConnection = nil
    end
    if afkConnection then
        afkConnection:Disconnect()
        afkConnection = nil
    end
    speedSlider.Text = "16"
    jumpSlider.Text = "50"
    speedLabel.Text = "Speed: 16"
    jumpLabel.Text = "Jump: 50"
    playerFrame.Visible = false
    resetCamera()
    
    local btn = homeContent:FindFirstChild("AntiAFKButton")
    if btn then
        btn.Text = "âš¡ Anti-AFK"
        TweenService:Create(btn, TweenInfo.new(0.3), {
            BackgroundColor3 = Color3.new(1, 1, 1)
        }):Play()
    end
    
    for _, part in pairs(character:GetChildren()) do
        if part:IsA("BasePart") and part.Name ~= "HumanoidRootPart" then
            part.CanCollide = true
        end
    end
end)

